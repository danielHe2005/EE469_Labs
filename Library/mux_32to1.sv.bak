// 32 to 1 mux that chooses between one of thirty two choices in a 32 element array "in" depending on the sel input
module mux_32to1 #(parameter width = 1) (in, sel, out);
	input logic [width-1:0] in [31:0];
	input logic [4:0] sel;
	output logic [width-1:0] out;
	logic interOne, interTwo;
	mux_8to1 #(width) muxL0P1 (.in(in[31:16]), .sel(sel[3:0]), .out(interTwo)); // chooses between input 31-16 depending on the bottom 4 bit of sel
	mux_8to1 #(width) muxL0P2 (.in(in[15:0]), .sel(sel[3:0]), .out(interOne)); // chooses between input 15-0 depending on the bottom 4 bit of sel
	mux_2to1 #(width) muxL1P1 (.in1(interTwo), .in0(interOne), .sel(sel[4]), .out(out)); // chooses between the outcome of the first two muxes depending on the top bit of sel
endmodule